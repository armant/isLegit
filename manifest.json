{
  "name": "is legit?",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Detects the legit firms on PennLink.",
  "browser_action": {
    "default_icon": "icon.png"
  },
    
  "content_scripts": [
    {
      "matches": ["https://upenn-csm.symplicity.com/*"],
      "js": ["jquery-2.1.1.min.js", "notable.js"]
    }
  ],
    
  "permissions": ["https://upenn-csm.symplicity.com/", "tabs"],
  "web_accessible_resources": ["icon.png"]
}